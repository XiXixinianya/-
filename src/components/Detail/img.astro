---
interface Props {
    title: string;
    num: number;
}

const { title, num } = Astro.props;
const img = `../static/images/${title}/${num}.jpg`;
---

<img src={img} alt="" />

<style>
    img {
        width: 100%;
        object-fit: fill;
        object-position: center;
    }
</style>
